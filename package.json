{
  "name": "webpack-client-ssr-store",
  "version": "1.0.0",
  "description": "First Complete project of webpack, client, ssr and store usage.",
  "main": "./src/simple.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "build:dev": "NODE_ENV=development webpack -d",
    "start": "npm run build && node ./src/server-es5.js",
    "start:dev": "export NODE_ENV=development webpack && nodemon --watch src ./src/server.js",
    "start:dev:universal": "export NODE_ENV=development && export UNIVERSAL=true && npm run start:dev",
    "start:universal": "export UNIVERSAL=true && npm run start"
  },
  "author": "Amit Kumar",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "ejs": "^2.6.1",
    "file-loader": "^3.0.1",
    "nodemon": "^1.18.9",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "express": "^4.16.4"
  }
}
